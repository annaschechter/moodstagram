<% @picture.errors.full_messages.each do |msg| %>
	<%= msg %>
<% end %>


<%= form_for @picture, :html => { :multipart => true } do |f| %>
	<%= f.label :image %>
	<%= f.file_field :image %>
	<%= f.label :feeling %>
	<%= f.text_field :feeling %>
  <%= f.label :price %>
  <%= f.text_field :price %>
	<%= f.submit %>
<% end %>